# Single threaded asynchronous ($sync) sequential writes (1MB I/Os) to
# a 1GB file.
# Stops after 1 series of 1024 ($count) writes has been done.

set $dir=/mnt/fuse
set $cached=false
set $count=1024
set $iosize=1m
set $nthreads=1
set $sync=false

set mode quit alldone

